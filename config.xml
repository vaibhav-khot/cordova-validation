<?xml version='1.0' encoding='utf-8'?>
<widget id="com.vaibhav.validation" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Vaibhav-Validation</name>
    <description>
        Validation-App
    </description>
    <author email="vaibhav.khot@neosofttech.com" href="http://cordova.io">
        Neosoft Team
    </author>
    <icon src="www/img/background.jpg" platform="android" width="57" height="57" density="mdpi" />
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
